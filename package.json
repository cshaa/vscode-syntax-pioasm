{
    "name": "pioasm",
    "displayName": "PIO ASM Syntax Highlighting",
    "description": "Syntax highlighting for Raspberry Pi Pico PIO assembly",
    "version": "1.0.0",
    "icon": "images/pico-asm.png",
    "publisher": "chris-hock",
    "repository": {
        "url": "https://gitlab.com/khockuba/vscode-syntax-pioasm"
    },
    "engines": {
        "vscode": "^1.53.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "pio",
            "aliases": ["PIO Assembly", "pio"],
            "extensions": [".pio"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "pio",
            "scopeName": "source.pio",
            "path": "./syntaxes/pio.tmLanguage.json"
        }]
    }
}